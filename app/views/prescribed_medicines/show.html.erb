<div class="page-header">
  <%= link_to prescribed_medicines_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_prescribed_medicine_path(@prescribed_medicine), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show prescribed_medicine</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @prescribed_medicine.name %></dd>

  <dt>Strength:</dt>
  <dd><%= @prescribed_medicine.strength %></dd>

  <dt>Dosage:</dt>
  <dd><%= @prescribed_medicine.dosage %></dd>

  <dt>Duration:</dt>
  <dd><%= @prescribed_medicine.duration %></dd>

  <dt>Note:</dt>
  <dd><%= @prescribed_medicine.note %></dd>

  <dt>Prescription:</dt>
  <dd><%= @prescribed_medicine.prescription %></dd>

</dl>
