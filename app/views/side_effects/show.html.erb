<div class="page-header">
  <%= link_to side_effects_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_side_effect_path(@side_effect), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show side_effect</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @side_effect.name %></dd>

  <dt>Description:</dt>
  <dd><%= @side_effect.description %></dd>

</dl>
